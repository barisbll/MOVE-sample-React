{
  "name": "sample-app-move",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package && react-native setup-ios-permissions",
    "prebuild": "expo prebuild --clean"
  },
  "reactNativePermissionsIOS": [
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse",
    "Motion"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "expo": "~48.0.18",
    "expo-build-properties": "~0.5.2",
    "expo-constants": "^14.2.1",
    "expo-device": "^5.2.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-move-sdk": "^2.4.0",
    "react-native-permissions": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.27",
    "typescript": "^4.9.4"
  },
  "private": true
}
